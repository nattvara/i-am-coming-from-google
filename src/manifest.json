{
    "description": "modify requests headers to add a referer header from google.com",
    "manifest_version": 2,
    "name": "i-am-coming-from-google",
    "version": "1.0",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*/*",
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": [
            "main.js"
        ]
    },
    "applications": {
        "gecko": {
            "id": "i-am-coming-from-google@kristoffersson.org"
        }
    },
    "browser_action": {
        "default_icon": {
            "38": "icons/btn-inactive.png"
        }
    }
}
